{
    "name": "ts-svgmanager",
    "version": "2.0.0-beta3",
    "description": "A Typescript SVG Manager",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "mocha -r ts-node/register -r jsdom-global/register tests/**/*.test.ts",
        "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
        "docs": "typedoc --out docs src; echo '' > ./docs/.nojekyll",
        "lint": "eslint ./src/**/*.ts",
        "lint:tests": "eslint ./tests/**/*.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/coastalwhite/ts-svgmanager.git"
    },
    "author": "Gijs Burghoorn",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/coastalwhite/ts-svgmanager/issues"
    },
    "homepage": "https://github.com/coastalwhite/ts-svgmanager#readme",
    "dependencies": {
        "ts-md5": "^1.2.7",
        "uuid": "^8.3.0"
    },
    "devDependencies": {
        "@types/chai": "^4.2.12",
        "@types/mocha": "^8.0.3",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.2.0",
        "@typescript-eslint/parser": "^4.2.0",
        "chai": "^4.2.0",
        "eslint": "^7.9.0",
        "eslint-plugin-unused-imports": "^0.1.3",
        "jsdom": "16.4.0",
        "jsdom-global": "3.0.2",
        "mocha": "^8.1.3",
        "nyc": "^15.1.0",
        "ts-node": "^9.0.0",
        "typedoc": "^0.19.1",
        "typescript": "^4.0.2"
    }
}
